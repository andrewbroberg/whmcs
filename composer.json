{
    "name": "andrewbroberg/whmcs",
    "description": "WHMCS PHP SDK",
    "keywords": [
        "andrewbroberg",
        "whmcs"
    ],
    "homepage": "https://github.com/andrewbroberg/whmcs",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Broberg",
            "email": "andrew@broberg.id.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/collections": "^12.0",
        "saloonphp/saloon": "^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "pestphp/pest": "^3.0",
        "phpstan/phpstan": "^2.1",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "AndrewBroberg\\WHMCS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AndrewBroberg\\WHMCS\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
